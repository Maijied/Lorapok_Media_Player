{
  "name": "lorapok-player",
  "private": true,
  "version": "1.1.0",
  "description": "A cross-platform media player with Organic Intelligence aesthetic.",
  "author": "Lorapok Team",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:linux": "ELECTRON_DISABLE_SANDBOX=1 vite",
    "dev:electron": "vite build && electron . --no-sandbox",
    "build": "tsc && vite build && electron-builder",
    "build:only": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "clsx": "^2.1.0",
    "dashjs": "^5.1.1",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^11.0.8",
    "hls.js": "^1.6.15",
    "lucide-react": "^0.358.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}